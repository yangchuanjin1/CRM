<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.ycj.dao.RolemodulesMapper">
  
  <!--  -->
  <delete id="delectRolesModules" parameterType="int">
  	delete from rolemodules where RM_RolesID=#{rolesId}
  </delete>
  <!--  -->
  <insert id="insertRolesModulesYang" parameterType="rolemodules">
  	insert into rolemodules(RM_RolesID,RM_ModulesID) value(#{RM_RolesID},#{RM_ModulesID})
  </insert>
  
  
  
  </mapper>